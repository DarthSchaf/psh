{
    "name": "shopware/psh",
    "description": "A build tool written in ph utilizing sh scripts",
    "type": "application",
    "autoload": {
        "psr-4": {
            "Shopware\\Psh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shopware\\Psh\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7",
        "symfony/finder": "^3.1",
        "symfony/process": "^3.1",
        "symfony/filesystem": "^3.1",
        "symfony/yaml": "^3.1",
        "league/climate": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Jan Pietrzyk",
            "email": "jp@shopware.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
